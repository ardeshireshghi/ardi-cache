{
    "extends": "airbnb-base",
    "plugins": [
      "import"
    ],
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-underscore-dangle": 0,
      "no-param-reassign": 0,
      "no-console": 0,
      "class-methods-use-this": 0,
      "no-confusing-arrow": 0,
      "no-plusplus": 0
    }
}
